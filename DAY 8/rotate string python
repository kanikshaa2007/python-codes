class Solution(object):
    def rotateString(self, s, goal):
        """
        :type s: str
        :type goal: str
        :rtype: bool
        """
        n=len(s)
        i=0
        while i<len(s):
            if goal==s:
                return True
            d=s[0]
            s=s[1:]+d
            i+=1
        return False
